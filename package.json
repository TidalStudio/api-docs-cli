{
  "name": "@tidalstudios/api-docs-cli",
  "version": "1.0.5",
  "description": "CLI tool for extracting API documentation",
  "type": "module",
  "bin": {
    "api-docs": "./bin/api-docs.js"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "start": "node bin/api-docs.js",
    "release": "node release.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test": "node --test",
    "test:smoke": "node bin/api-docs.js --help"
  },
  "keywords": [
    "cli",
    "api",
    "documentation",
    "openapi",
    "swagger"
  ],
  "author": "Logan McDuffie",
  "license": "Elastic-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TidalStudio/api-docs-cli.git"
  },
  "bugs": {
    "url": "https://github.com/TidalStudio/api-docs-cli/issues"
  },
  "homepage": "https://github.com/TidalStudio/api-docs-cli#readme",
  "files": [
    "bin/",
    "src/"
  ],
  "dependencies": {
    "chalk": "^5.6.2",
    "dotenv": "^17.2.3",
    "js-yaml": "^4.1.0",
    "puppeteer": "^24.34.0",
    "yargs": "^18.0.0"
  },
  "devDependencies": {
    "eslint": "^9.17.0",
    "prettier": "^3.4.2"
  }
}
